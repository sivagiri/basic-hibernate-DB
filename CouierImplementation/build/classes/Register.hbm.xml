<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.siva.model.Register" table="register">
		<id name="id">
			<column name="id" />
			<generator class="native" />
		</id>

		<property name="username">
			<column name="username" />
		</property>

		<property name="password">
			<column name="password" />
		</property>
		
		<property name="fname">
			<column name="fname" />
		</property>
		
		<property name="departmentid">
			<column name="departmentid" />
		</property>

		<property name="designationid">
			<column name="designationid" />
		</property>

		<property name="role">
			<column name="role" />
		</property>

		<property name="email">
			<column name="email" />
		</property>

		<property name="mobileno">
			<column name="mobileno" />
		</property>

	</class>
</hibernate-mapping>
